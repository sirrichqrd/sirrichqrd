{
  "id": "/",
  "name": "SIR.RICHQRD",
  "short_name": "Richqrd",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#fff",
  "theme_color": "#fff",
  "icons": [
  {
    "src": "/icon-192.png",
    "sizes": "192x192",
    "type": "image/png"
  },
  {
    "src": "/icon-512.png",
    "sizes": "512x512",
    "type": "image/png"
  }
],
  "screenshots": [
    {
      "src": "/home.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "/mobile.png",
      "sizes": "640x1136",
      "type": "image/png"
    }
  ],
   "protocol_handlers": [
    {
      "protocol": "web+bookrichqrd",
      "url": "/?action=book&q=%s"
    },
    {
      "protocol": "web+contactrichqrd",
      "url": "/?action=contact&q=%s"
    }
  ],
  "display_override": ["standalone", "window-controls-overlay"]
}
